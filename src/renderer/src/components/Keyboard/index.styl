.kb-container {
    position: relative;
    overflow: hidden;
    background-color: #FFF;

    .kb {
        position: relative;
    }

    .kb-w {
        flex: 1;
        height: 100%;
        box-shadow: inset 0 -1px 2px 1px #333;
    }

    .kb-b {
        position: absolute;
        width: 60%;
        height: 75%;
        top: -10px;
        left: 70%;
        border-radius: 2px;
        z-index: 1;
        box-shadow: inset 0px 0px 2px 2px #666;
    }
}